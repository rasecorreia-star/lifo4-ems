{
  "name": "@lifo4/shared",
  "version": "1.0.0",
  "description": "Shared types, constants, and utilities for LIFO4 EMS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "lifo4",
    "ems",
    "shared",
    "types",
    "constants"
  ],
  "author": "Lifo4 Energia",
  "license": "UNLICENSED",
  "devDependencies": {
    "typescript": "^5.3.2"
  }
}
